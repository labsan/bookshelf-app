{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "require-jsdoc": 0,
        "camelcase": 0,
        "no-unused-vars": 0,
        "code": 100
    }
}
